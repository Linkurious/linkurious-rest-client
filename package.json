{
  "name": "linkurious-rest-client",
  "version": "2.2.1",
  "description": "A TypeScript lib to interact with Linkurious REST API",
  "contributors": [
    {
      "name": "Maxime Allex",
      "email": "maxime@linkurio.us",
      "url": "https://github.com/maxime-allex"
    },
    {
      "name": "David Rapin",
      "email": "david@linkurio.us",
      "url": "https://github.com/davidrapin"
    },
    {
      "name": "Andr√© Barata",
      "email": "andre@linkurio.us",
      "url": "https://github.com/aabarata"
    }
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Linkurious/linkurious-rest-client.git"
  },
  "author": "Linkurious",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Linkurious/linkurious-rest-client/issues"
  },
  "homepage": "https://github.com/Linkurious/linkurious-rest-client#readme",
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "compileTests": "tsc --project tsconfig_tests.json",
    "test": "./test/runTests.sh",
    "posttest": "rm -rf tempTests",
    "clean": "rm -rf ./dist-es6 && rm -rf ./typings && rm -rf ./node_modules && rm -rf ./coverage",
    "bundling": "browserify dist-es6/index.js -s Linkurious --outfile bundle.js",
    "compile": "babel bundle.js --out-file linkurious-rest-client-es5.js",
    "build": "tsc && npm run bundling && npm run compile && rm bundle.js",
    "doc": "typedoc --ignoreCompilerErrors --target ES6 --out ./docs index.ts",
    "prepublishOnly": "tsc -p ./tsconfig.json",
    "patch": "npm version patch && git push && npm publish"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.35",
    "@types/node": "^8.0.10",
    "@types/superagent": "^3.5.2",
    "browserify": "^13.0.1",
    "bunyan": "^1.8.1",
    "es6-module-loader": "^0.17.11",
    "fs-extra": "^0.30.0",
    "istanbul": "^0.4.3",
    "jasmine-core": "2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^2.2.0",
    "karma-clear-screen-reporter": "^1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-html-reporter": "^0.2.7",
    "karma-jasmine": "^0.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.1.0",
    "karma-systemjs": "^0.13.0",
    "path": "^0.12.7",
    "request": "~2.72",
    "systemjs": "^0.19.29",
    "tslint": "^3.10.2",
    "typedoc": "0.9.0",
    "typescript": "2.6.2"
  },
  "dependencies": {
    "superagent": "^3.5.2"
  }
}
