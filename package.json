{
  "name": "linkurious-rest-client",
  "version": "2.7.0",
  "description": "A TypeScript lib to interact with Linkurious REST API",
  "contributors": [
    {
      "name": "Maxime Allex",
      "email": "maxime@linkurio.us",
      "url": "https://github.com/maxime-allex"
    },
    {
      "name": "David Rapin",
      "email": "david@linkurio.us",
      "url": "https://github.com/davidrapin"
    },
    {
      "name": "Andr√© Barata",
      "email": "andre@linkurio.us",
      "url": "https://github.com/aabarata"
    }
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Linkurious/linkurious-rest-client.git"
  },
  "author": "Linkurious",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Linkurious/linkurious-rest-client/issues"
  },
  "homepage": "https://github.com/Linkurious/linkurious-rest-client#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "compileTests": "tsc --project fetcher_spec.json",
    "test": "./test/runTests.sh",
    "posttest": "rm -rf tempTests",
    "clean": "rm -rf ./dist-es6 && rm -rf ./typings && rm -rf ./node_modules && rm -rf ./coverage",
    "bundling": "browserify dist-es6/index.js -s Linkurious --outfile bundle.js",
    "compile": "babel bundle.js --out-file linkurious-rest-client-es5.js",
    "build": "tsc",
    "doc": "typedoc --ignoreCompilerErrors --target ES6 --out ./docs index.ts",
    "prepublishOnly": "tsc -p ./tsconfig.json",
    "minor": "tsc -p ./tsconfig.json && npm version minor && git push --tags && npm publish",
    "patch": "tsc -p ./tsconfig.json && npm version patch && git push --tags && npm publish",
    "prerelease": "tsc -p ./tsconfig.json && npm version prerelease && git push && npm publish --tag next"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/superagent": "^3.5.2",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "fs-extra": "^0.30.0",
    "husky": "^1.2.0",
    "karma": "^4.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^4.0.0",
    "karma-typescript-es6-transform": "^4.0.0",
    "mocha": "^5.2.0",
    "path": "^0.12.7",
    "prettier": "^1.13.5",
    "pretty-quick": "^1.8.0",
    "request": "^2.87.0",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.0.0-rc.0",
    "tslint": "^3.10.2",
    "typescript": "3.1.6"
  },
  "peerDependencies": {
    "linkurious-shared": "github:Linkurious/builds#shared-develop-next"
  },
  "dependencies": {
    "linkurious-shared": "github:Linkurious/builds#shared-develop-next",
    "superagent": "^3.5.2",
    "valcheck": "^1.1.0"
  }
}
